{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "types": ["astro/client", "react", "react-dom"],
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
